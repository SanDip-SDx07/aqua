{
  "name": "aqua",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "backend"
  ],
  "scripts": {
    "dev:utils": "yarn --cwd packages/utils dev",
    "dev:shared": "yarn --cwd packages/shared dev",
    "dev:backend": "yarn --cwd backend start:dev",
    "dev:public": "yarn --cwd apps/public dev",
    "dev": "concurrently --kill-others \"yarn dev:utils\" \"yarn dev:backend\"",
    "build:utils": "yarn --cwd packages/utils build",
    "build:backend": "yarn --cwd backend build",
    "build": "yarn build:utils && yarn build:backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SanDip-SDx07/aqua.git"
  },
  "keywords": [],
  "author": "Sandip Das",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/SanDip-SDx07/aqua/issues"
  },
  "homepage": "https://github.com/SanDip-SDx07/aqua#readme",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
